<div class="form">

  <%= form_for @order do |f| %>
  <%= f.label :table %><br>
  <%= collection_select(:order, :table_id, @tables, :id, :guests, :prompt => true) %><br><br>

  <%= f.label :fooditem, {:multiple => true} %>
  <%= collection_select(:fooditem_order, :fooditem_id, @fooditems, :id, :itemname, :prompt => true) %>

  <%= f.label :cooked %>
  <%= f.check_box :cooked, {}, true, false %> <br>

  <%= f.submit "Add Order" %>

  <% end %>
</div>
